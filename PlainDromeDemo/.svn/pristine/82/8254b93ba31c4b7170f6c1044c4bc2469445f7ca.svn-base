public class CheckPlainDromeCase {

	public static void main(String[] args) {

		String str = "hello";

		String str1 = "";

		for (int i = str.length() - 1; i > 0; i--) {

			str1 = str1 + str.charAt(i);

		}

		if (str.equals(str1)) {

			System.out.println("this is plaindrome");
		} else
			System.out.println("this is not plaindrome");

	}
}