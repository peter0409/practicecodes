package com.nivtek.corejava.operators.lab2;

/**
 * @author Bootcamp User 017
 *
 */
public class WeightConverter {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		double weight_earth = 50;
		double weight_moon = getWeightOnMoon(weight_earth);
		System.out.println("weight on moon is " + weight_moon);

	}

	/**
	 * @param weight_earth
	 * @return weight_moon
	 */
	public static double getWeightOnMoon(Double weight_earth) {
		double weight_moon = 0.17 * weight_earth;
		return weight_moon;
	}

}
